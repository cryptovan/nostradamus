{
    "name": "nostradamus",
    "version": "0.1.0",
    "repository": "git@github.com:ericmuyser/nostradamus.git",
    "license": "UNLICENSED",
    "main": "build/index.js",
    "scripts": {
        "build": "yarn run tsc --listEmittedFiles",
        "format": "./node_modules/.bin/tsfmt -r src/index.ts",
        "start": "yarn run format && yarn run build && yarn run babel-node build/index.js",
        "electron": "tsc && ELECTRON_ENABLE_LOGGING=1 electron ."
    },
    "dependencies": {
        "babel-cli": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "isomorphic-fetch": "^2.2.1",
        "node-fetch": "^1.7.1",
        "querystring": "^0.2.0",
        "typescript": "^2.3.2",
        "typescript-formatter": "^5.2.0"
    },
    "engines": {
        "node": "7.4.0",
        "npm": "3.10.10"
    },
    "devDependencies": {
        "electron": "1.6.11",
        "typings": "^2.1.1"
    }
}
